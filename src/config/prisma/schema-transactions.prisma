generator client {
  provider = "prisma-client-js"
  output   = "./generated/transactions"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_TRANSACTION")
}

model Transactions {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  userId String?
  agentId String?
  type         String
  method       String?
  gameId       String?
  updateUserUsername String?
  amount       Float     @default(0.0)
  note         String?
  token        String?
  status       String?
  currencyCode   String?
  createdAt    DateTime  @default(now())
  updatedAt    DateTime? @updatedAt
  deletedAt    DateTime?
}

